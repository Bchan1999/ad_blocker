{
    "name": "Ad Blocker",
    "version": "2.0.0",
    "description": "The only Youtube Premium you'll ever need",
    "manifest_version": 3,
    "permissions": [
        "declarativeNetRequest"
    ],
    "declarative_net_request": {
        "rule_resources": [
            {
                "id": "ruleset_1",
                "enabled": true,
                "path": "rules.json"
            }
        ]
    }
}